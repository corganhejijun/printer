<Window x:Class="Wpf3DPrint.Dialog.DisplaySetting"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf3DPrint.Dialog"
        mc:Ignorable="d"
        Title="显示设置" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="textBoxColor" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="(156, 156, 156)" VerticalAlignment="Top" Width="120" IsEnabled="False"/>
        <Button x:Name="buttonSelectColor" Content="选择颜色..." HorizontalAlignment="Left" Margin="135,10,0,0" VerticalAlignment="Top" Width="75" Click="buttonSelectColor_Click"/>

    </Grid>
</Window>
